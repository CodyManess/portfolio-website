    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9",
    "eslint-config-next": "^15",
    "eslint-config-prettier": "^9.1.0",
